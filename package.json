{
  "name": "storyboard-vscode-extension",
  "displayName": "Crank Storyboard",
  "description": "Crank Storyboard VSCode Snippets",
  "publisher": "jake-butterfield",
  "icon": "images/logo.png",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/extension",
  "contributes": {
    "snippets": [
      {
        "language": "lua",
        "path": "./src/snippets/gre.json"
      },
	  {
        "language": "lua",
        "path": "./src/snippets/gredom.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "repository": "https://github.com/JakeButterfield99/storyboard-vscode-extension/"
}
